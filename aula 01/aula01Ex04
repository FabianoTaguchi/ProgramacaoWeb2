x = int(input('Digite um valor para o eixo X: '))
y = int(input('Digite um valor para o eixo Y: '))

if x>0 and y>0:
    print('1ยบ quadrante')
elif x<0 and y>0:
    print('2ยบ quadrante')
elif x<0 and y<0:
    print('3ยบ quadrante')
elif x>0 and y<0:
    print('4 quadrante')
else:
    print('Origem')